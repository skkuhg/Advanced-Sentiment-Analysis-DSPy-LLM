    "# Install required packages if not already installed\\n",
    "import subprocess\\n",
    "import sys\\n",
    "\\n",
    "def install_if_missing(package):\\n",
    "    try:\\n",
    "        __import__(package.split('>=')[0].replace('-', '_'))\\n",
    "    except ImportError:\\n",
    "        print(f\\"Installing {package}...\\")\\n",
    "        subprocess.check_call([sys.executable, '-m', 'pip', 'install', package])\\n",
    "\\n",
    "# Core dependencies\\n",
    "packages = [\\n",
    "    'dspy-ai>=2.4.0',\\n",
    "    'openai>=1.0.0',\\n",
    "    'pandas>=1.5.0',\\n",
    "    'numpy>=1.21.0',\\n",
    "    'matplotlib>=3.5.0',\\n",
    "    'seaborn>=0.11.0',\\n",
    "    'plotly>=5.0.0',\\n",
    "    'scikit-learn>=1.1.0',\\n",
    "    'python-dotenv>=0.19.0'\\n",
    "]\\n",
    "\\n",
    "for package in packages:\\n",
    "    install_if_missing(package)\\n",
    "\\n",
    "print(\\"‚úÖ All dependencies installed successfully!\\")\\n",
    "print(\\"üìã Next: Run the setup cell to configure your environment\\")\\n"
